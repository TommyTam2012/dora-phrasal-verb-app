{
  "version": 2,
  "headers": [
    {
      "source": "/exam/(.*)\\.pdf",
      "headers": [
        { "key": "Content-Type", "value": "application/pdf" },
        { "key": "Content-Disposition", "value": "inline" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
